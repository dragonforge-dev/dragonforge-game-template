; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Dragonforge Game Template v0.3.2"
config/description="A game template for game jams."
config/version="0.3.2"
run/main_scene="res://addons/dragonforge_game_template/main.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://addons/dragonforge_game_template/assets/textures/logo/icon_monochrome_dark.svg"

[autoload]

Gamepad="*res://addons/dragonforge_controller/gamepad.tscn"
Mouse="*res://addons/dragonforge_controller/mouse.tscn"
Keyboard="*res://addons/dragonforge_controller/keyboard.tscn"
Controller="*res://addons/dragonforge_controller/controller.tscn"
Display="*res://addons/dragonforge_display/display.tscn"
Sound="*res://addons/dragonforge_sound/sound.tscn"
Music="*res://addons/dragonforge_sound/music.tscn"
Disk="*res://addons/dragonforge_disk/disk.tscn"
Game="*res://addons/dragonforge_game_template/game.tscn"

[editor_plugins]

enabled=PackedStringArray("res://addons/dragonforge_controller/plugin.cfg", "res://addons/dragonforge_disk/plugin.cfg", "res://addons/dragonforge_display/plugin.cfg", "res://addons/dragonforge_game_template/plugin.cfg", "res://addons/dragonforge_sound/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/dragonforge_game_template/": "red"
}

[global_group]

Persist="Group for all nodes that save/load game data to disk when game is saved/loaded."

